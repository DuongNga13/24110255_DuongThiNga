#include <bits/stdc++.h>
using namespace std;

int main(){
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int N, M;
    cin >> N >> M;

    vector<vector<pair<int,int>>> adj(N+1);
    for(int i=0;i<M;i++){
        int u,v,w;
        cin >> u >> v >> w;
        adj[u].push_back({v,w});
        adj[v].push_back({u,w}); // nếu đồ thị có hướng, bỏ dòng này
    }

    const long long INF = 1e18;
    vector<long long> dist(N+1, INF);
    priority_queue<pair<long long,int>, vector<pair<long long,int>>, greater<>> pq;

    dist[1] = 0;
    pq.push({0,1});

    while(!pq.empty()){
        auto [cost,u] = pq.top(); pq.pop();
        if(cost > dist[u]) continue;

        for(auto [v,w] : adj[u]){
            if(dist[v] > cost + w){
                dist[v] = cost + w;
                pq.push({dist[v], v});
            }
        }
    }

    for(int i=1;i<=N;i++)
        cout<<(dist[i]==INF?-1:dist[i])<<" ";
}
